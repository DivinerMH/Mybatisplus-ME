<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace=".mapper.EventModelMapper">
    <!-- 结果集 -->
    <resultMap type=".entity.EventModel" id="EventModelMap">
        <result property="id" column="id" jdbcType="INTEGER"/>
        <result property="dangerousSourcesTypeId" column="dangerous_sources_type_id" jdbcType="INTEGER"/>
        <result property="userId" column="user_id" jdbcType="INTEGER"/>
        <result property="planId" column="plan_id" jdbcType="INTEGER"/>
        <result property="modelName" column="model_name" jdbcType="VARCHAR"/>
        <result property="alarmId" column="alarm_id" jdbcType="INTEGER"/>
        <result property="createTime" column="create_time" jdbcType="TIMESTAMP"/>
        <result property="updateTime" column="update_time" jdbcType="TIMESTAMP"/>
        <result property="delFlag" column="del_flag" jdbcType="VARCHAR"/>
    </resultMap>

    <!-- 基本字段 -->
    <sql id="Base_Column_List">id, dangerous_sources_type_id, user_id, plan_id, model_name, alarm_id, create_time, update_time, del_flag</sql>
</mapper>